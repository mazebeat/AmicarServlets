<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>AmicarSvl</display-name>
    <servlet>
        <servlet-name>Lecturas</servlet-name>
        <servlet-class>cl.intelidata.amicarsvl.servlet.Lecturas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Clicks</servlet-name>
        <servlet-class>cl.intelidata.amicarsvl.servlet.Clicks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Desinscritos</servlet-name>
        <servlet-class>cl.intelidata.amicarsvl.servlet.Desinscritos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Lecturas</servlet-name>
        <url-pattern>/Lecturas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Clicks</servlet-name>
        <url-pattern>/Clicks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Desinscritos</servlet-name>
        <url-pattern>/Desinscritos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
